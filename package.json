{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.14.0",
    "npm": ">=10.9.2"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "commit": "git-cz",
    "format": "NODE_OPTIONS=\"--experimental-strip-types\" prettier --write .",
    "prepare": "node .husky/install.mjs",
    "release": "standard-version",
    "typecheck": "tsc --noEmit",
    "migration:down": "knex migrate:down --knexfile ./knexfile.cjs",
    "migration:latest": "knex migrate:latest --knexfile ./knexfile.cjs",
    "migration:new": "knex migrate:make $1 --migrations-directory src/database/migrations --stub ./migration.stub.cjs",
    "migration:rollback": "knex migrate:rollback --knexfile ./knexfile.cjs",
    "migration:up": "knex migrate:up --knexfile ./knexfile.cjs"
  },
  "type": "module",
  "imports": {
    "#validators/*": [
      "./src/validators/*.js"
    ],
    "#database/*": [
      "./src/database/*.js"
    ],
    "#services/*": [
      "./src/services/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-accordion": "^1.2.7",
    "@radix-ui/react-alert-dialog": "^1.1.10",
    "@radix-ui/react-aspect-ratio": "^1.1.4",
    "@radix-ui/react-avatar": "^1.1.6",
    "@radix-ui/react-checkbox": "^1.2.2",
    "@radix-ui/react-collapsible": "^1.1.7",
    "@radix-ui/react-context-menu": "^2.2.11",
    "@radix-ui/react-dialog": "^1.1.10",
    "@radix-ui/react-dropdown-menu": "^2.1.11",
    "@radix-ui/react-hover-card": "^1.1.10",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-menubar": "^1.1.11",
    "@radix-ui/react-navigation-menu": "^1.2.9",
    "@radix-ui/react-popover": "^1.1.10",
    "@radix-ui/react-progress": "^1.1.4",
    "@radix-ui/react-radio-group": "^1.3.3",
    "@radix-ui/react-scroll-area": "^1.2.5",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slider": "^1.3.2",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.8",
    "@radix-ui/react-toast": "^1.2.10",
    "@radix-ui/react-toggle": "^1.1.6",
    "@radix-ui/react-toggle-group": "^1.1.7",
    "@radix-ui/react-tooltip": "^1.2.3",
    "@vinejs/vine": "^3.0.1",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "embla-carousel-react": "^8.6.0",
    "express": "^5.1.0",
    "input-otp": "^1.4.2",
    "knex": "^3.1.0",
    "lucide-react": "^0.503.0",
    "next": "^15.3.1",
    "next-sitemap": "^4.2.3",
    "next-themes": "^0.4.6",
    "pg": "^8.14.1",
    "pg-connection-string": "^2.7.0",
    "react": "^19.1.0",
    "react-day-picker": "^9.6.7",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.0",
    "react-resizable-panels": "^2.1.8",
    "recharts": "^2.15.3",
    "sequelize": "^6.37.7",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vaul": "^1.1.2",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/compat": "^1.2.8",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@next/eslint-plugin-next": "^15.3.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@stylistic/eslint-plugin-jsx": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.6.2",
    "@types/node": "^22.14.1",
    "@types/pg": "^8.11.13",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "git-cz": "^4.9.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-edgejs": "^1.0.0",
    "sequelize-auto": "^0.8.8",
    "sort-package-json": "^3.0.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1"
  }
}
